    <title>管理系统</title>

    {% block stylesheets %}
    <link rel="Bookmark" href="/favicon.ico" >
    <link rel="Shortcut Icon" href="/favicon.ico" />
    <!--[if lt IE 9]>
    <script type="text/javascript" src="{{ asset('AdminAssets/lib/') }}html5shiv.js"></script>
    <script type="text/javascript" src="{{ asset('AdminAssets/lib/') }}respond.min.js"></script>
    <![endif]-->
    <link rel="stylesheet" type="text/css" href="{{ asset('AdminAssets/static/') }}h-ui/css/H-ui.min.css" />
    <link rel="stylesheet" type="text/css" href="{{ asset('AdminAssets/static/') }}h-ui.admin/css/H-ui.admin.css" />
